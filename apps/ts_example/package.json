{
  "name": "@canonical/ds25-ts-test",
  "private": true,
  "version": "0.3.1-experimental.0",
  "description": "Typescript test project",
  "type": "module",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "author": {
    "email": "webteam@canonical.com",
    "name": "Canonical Webteam"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/canonical/ds25"
  },
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/canonical/ds25/issues"
  },
  "homepage": "https://github.com/canonical/ds25#readme",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "bun run lint:biome",
    "lint:fix": "bun run lint:biome:fix",
    "lint:biome": "biome lint src *.json",
    "lint:biome:fix": "biome lint --write src*.json",
    "format": "bun run format:biome",
    "format:fix": "bun run format:biome:fix",
    "format:biome": "biome format src *.json",
    "format:biome:fix": "biome format --write src *.json",
    "check": "bun run check:biome && bun run check:ts",
    "check:fix": "bun run check:biome:fix",
    "check:biome": "biome check src *.json",
    "check:biome:fix": "biome check --write src *.json",
    "check:ts": "tsc --noEmit"
  },
  "dependencies": {
    "@canonical/utils": "^0.3.1-experimental.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@canonical/biome-config": "^0.3.1-experimental.0",
    "@canonical/typescript-config-base": "^0.3.1-experimental.0"
  }
}
