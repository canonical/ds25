{
  "error": {
    "$extensions": {
      "selector": ".error"
    },
    "intent-color": {
      "$value": "{color.negative-default}",
      "$type": "color",
      "$description": "Color representing error or danger intent"
    }
  },
  "success": {
    "$extensions": {
      "selector": ".success"
    },
    "intent-color": {
      "$value": "{color.positive-default}",
      "$type": "color",
      "$description": "Color representing success intent"
    }
  }
}
